 private void disableData() {
        try {
            if (!(admin == null)) {

            }
            if (!(manager == null)){
                allTabs.getTabs().remove(userManagementTab);

            }
            if (!(driver == null)){
                allTabs.getTabs().remove(userManagementTab);
                CreateDestinationButton.setDisable(true);
                CreateCargoButton.setDisable(true);
                CreateVehicleButton.setDisable(true);

                DeleteVehicleButton.setDisable(true);
                DestinationDeleteButton.setDisable(true);
                CargoDeleteButton.setDisable(true);
            }

        } catch (Exception e) {
            e.printStackTrace();
            //allTabs.getTabs().remove(userManagementTab);
        }
    }
public void DisableData(int Propertie){
        switch(Propertie){
            case 1:{

                break;

            }
            case 2:{

                break;
            }
            case 3:{
                Vname.setDisable(true);
                Vmodel.setDisable(true);
                Vdescription.setDisable(true);
                Vpower.setDisable(true);
                VfuelConsumption.setDisable(true);
                VMaxWeight.setDisable(true);
                VAcceptsOnly.setDisable(true);
                break;
            }
            default:{}
        }
    }
